{
  "service": {
    "name": "arcadia-monitor-all",
    "port": 81,
    "checks": [
      {
        "id": "main",
        "name": "nginx TCP Check",
        "tcp": "${LOCAL_IPV4}:81",
        "interval": "10s",
        "timeout": "1s"
      },
      {
        "id": "backend",
        "name": "nginx TCP Check",
        "tcp": "${LOCAL_IPV4}:84",
        "interval": "10s",
        "timeout": "1s"
      },
      {
        "id": "app2",
        "name": "nginx TCP Check",
        "tcp": "${LOCAL_IPV4}:82",
        "interval": "10s",
        "timeout": "1s"
      },
      {
        "id": "app3",
        "name": "nginx TCP Check",
        "tcp": "${LOCAL_IPV4}:83",
        "interval": "10s",
        "timeout": "1s"
      }
    ]
  }
}